<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.runy.logical_thinking.mapper.ExampleMapper">
    <select id="selectExample" resultType="jp.co.runy.logical_thinking.domain.Example">
		select 
			id,
			explanation,
			reason_id
		from
			examples
		where
			session_id = #{sessionId};
    </select>
</mapper>